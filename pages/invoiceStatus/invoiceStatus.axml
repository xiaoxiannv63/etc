  <view class="positionBottom">
    <view>
      <flex justify="between">
        <view class="w70Border inline" onTap="invoiceError">发票信息有误？请点击</view>
        <view class="w30Border inline" onTap="lookQuotation">查看行程</view>
      </flex>
    </view>
    <view  class="flex-container">  
        <flex class="flexItem">
          <flex-item>
            <view class="placeholder w30" > <image src="../../assets/images/fxCion.png"/> 分享</view>
            <view class="placeholder w70"><view class="primary" onTap="lookElectronicInvoice">查看电子发票</view></view>
          </flex-item>
        </flex>
    </view>
  </view>
</view>
