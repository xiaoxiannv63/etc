
<view class="page">
  <view class="statusHead">
    <view class="ul">
      <view class="li">申请类型： {{itemContent.applyType}}
      </view>
      <view class="li">发票内容： 通行费
      </view>
      <view class="li">发票张数： {{itemContent.sumInvoice}}张
      </view>
      <view class="li">行程总数： {{itemContent.countRecord}}个
      </view>
      <view class="li">申请时间： {{itemContent.applyTime}}
      </view>
      <view class="li">申请邮箱： {{itemContent.invoiceMail}}
      </view>
      <view class="amount">
        <view class="money">￥{{(itemContent.sumAmount/100).toFixed(2)}}
        </view>
        <view class="font">发票总金额
        </view>
      </view>
    </view> 
  </view>
  <view class="statusHead bgColorWhite">
    <view class="li">{{itemContent.buyerName}}</view>
    <view class="li">税号：{{itemContent.buyerTaxpayerCode}}</view>
  </view>
